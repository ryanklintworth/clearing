<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <%- include ('./partials/head.ejs') %>
  </head>
  <body>
    <header>HEADER</header>
    <div class="container">
      <ul>
        <% for (let item of items ) { %>
          <li>
            <h3><%=item.name%></h3>
            <a href="/<%=item._id%>">
              <img src="<%= item.img %>" alt="An image of a <%= item.name %>" />
            </a>
          </li>
        <% } %>
      </ul>
    </div>
    <footer>FOOTER</footer>
  </body>
</html>
